


<h1>Log in</h1>
<p>Find me in app/views/user/new.html.erb</p>

<div class="row">

  <div class="col-md-6 col-md-offset-3">
    <%= form_for @user, url: {action: "create"} do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>


      <%= f.submit "Log in & sign up" %>
    <% end %>
  </div>


</div>